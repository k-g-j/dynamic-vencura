# Environment Configuration
NODE_ENV=development
PORT=3001

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/vencura

# Security
JWT_SECRET=your-jwt-secret-key-min-32-characters-long-replace-this
ENCRYPTION_KEY=your-encryption-key-min-32-characters-long-replace-this

# Blockchain
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/63264d1583fd460d8aace681426f267c

# Dynamic Authentication
DYNAMIC_ENVIRONMENT_ID=your-dynamic-environment-id
DYNAMIC_API_KEY=your-dynamic-api-key-optional

# CORS
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info